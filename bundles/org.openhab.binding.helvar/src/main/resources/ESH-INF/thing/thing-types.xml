<?xml version="1.0" encoding="UTF-8"?>
<thing:thing-descriptions bindingId="helvar"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:thing="https://openhab.org/schemas/thing-description/v1.0.0"
	xsi:schemaLocation="https://openhab.org/schemas/thing-description/v1.0.0 https://openhab.org/schemas/thing-description-1.0.0.xsd">

	<!-- Sample Thing Type -->

	<bridge-type id="router">
		<label>Helvar Bridge</label>
		<description>This represents a Helvar Router</description>

		<properties>
			<property name="vendor">Helvar</property>
		</properties>

		<config-description>
			<parameter name="ipAddress" type="text">
				<context>network-address</context>
				<label>Network Address</label>
				<description>The network address of the helvar router.</description>
				<required>true</required>
			</parameter>
			<parameter name="port" type="integer" required="false" min="1" max="65535">
				<label>Port</label>
				<description>Port TCP port the Helvar router is expecting to recieve HelvarNet requests on.
				Default is 50000.</description>
				<default>50000</default>
			</parameter>
			<parameter name="clusterId" type="integer" required="false" min="0" max="253">
				<label>Cluster</label>
				<description>Router Cluster ID. It seems that this should be set to 0 if no clusters set.</description>
				<default>0s</default>
			</parameter>
			<parameter name="routerId" type="integer" required="false" min="1" max="254">
				<label>Router</label>
				<description>Router Router ID</description>
				<default>1</default>
			</parameter>

		</config-description>
	</bridge-type>

	<thing-type id="dimmer">
		<supported-bridge-type-refs>
			<bridge-type-ref id="router"/>
		</supported-bridge-type-refs>

		<label>Dimmer</label>
		<description>Controls dimmable things, probably lights.</description>

		<channels>
			<channel id="lightlevel" typeId="lightDimmer"/>
		</channels>

		<config-description>
			<parameter name="subnetId" type="integer" required="true" min="1" max="4">
				<label>Subnet</label>
				<description>Subnet ID of the dimmer under the assigned router.</description>
			</parameter>
			<parameter name="deviceId" type="integer" required="true" min="1" max="255">
				<label>Device</label>
				<description>Device ID of the dimmer under the assigned router.</description>
			</parameter>
		</config-description>
	</thing-type>

	<thing-type id="group">
		<supported-bridge-type-refs>
			<bridge-type-ref id="router"/>
		</supported-bridge-type-refs>

		<label>Group</label>
		<description>A group of things on the Helvar device. Groups support scenes, have a name and map directly to
			DALI groups on the DALI bus.</description>

